{
  "name": "@gaconkzk/flyui-vue3",
  "version": "0.2.3",
  "files": [
    "dist",
    "@types",
    "lib"
  ],
  "main": "./dist/flyui-vue3.umd.js",
  "module": "./dist/flyui-vue3.esm.js",
  "exports": {
    ".": {
      "import": "./dist/flyui-vue3.esm.js",
      "require": "./dist/flyui-vue3.umd.js"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./types": {
      "import": "./dist/types/flyui.d.ts"
    },
    "./core/*": {
      "import": "./lib/core/*",
      "require": "./lib/core/*"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@windicss/plugin-scrollbar": "^1.2.3",
    "color": "^4.0.1",
    "lodash": "^4.17.21",
    "vue": "^3.2.21"
  },
  "devDependencies": {
    "@types/color": "^3.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@vitejs/plugin-vue": "^1.9.4",
    "@windicss/plugin-animations": "^1.0.9",
    "@yzfe/svgicon": "^1.1.0",
    "@yzfe/vue3-svgicon": "^1.0.1",
    "sass": "^1.43.4",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vite-plugin-svg-icons": "^1.0.5",
    "vite-plugin-svgicon": "^1.0.0",
    "vite-plugin-windicss": "^1.5.1",
    "vue-tsc": "^0.28.10",
    "windicss": "^3.2.1"
  }
}
