{
  "name": "@gaconkzk/flyui-svelte",
  "version": "0.1.11",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/flyui-svelte.umd.js",
  "module": "./dist/flyui-svelte.esm.js",
  "svelte": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/flyui-svelte.esm.js",
      "require": "./dist/flyui-svelte.umd.js"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./types": {
      "import": "./dist/types/flyui.d.ts"
    },
    "./components/*": {
      "import": "./src/lib/*"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@gaconkzk/core": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.31",
    "@tsconfig/svelte": "^2.0.1",
    "@types/node": "^16.11.11",
    "@windicss/plugin-animations": "^1.0.9",
    "@yzfe/svgicon": "^1.1.0",
    "color": "^4.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.44.0",
    "svelte": "3.39.0",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.9.8",
    "svelte-windicss-preprocess": "^4.2.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-plugin-windicss": "^1.5.4",
    "windicss": "^3.2.1"
  }
}
