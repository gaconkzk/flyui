{
  "name": "flyui-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": ".prettierrc.js",
  "scripts": {
    "dev": "pnpm run dev --filter=\"@gaconkzk/flyui-vue3\"",
    "dev:svelte": "pnpm run dev --filter=\"@gaconkzk/flyui-svelte\"",
    "dev:react": "pnpm run dev --filter=\"@gaconkzk/flyui-react\"",
    "dev:docs": "pnpm run build --filter=\"@gaconkzk/flyui-vue3\" && pnpm run dev --filter=\"docs\"",
    "build": "pnpm build --filter=\"@gaconkzk/flyui-vue3\" --filter=\"@gaconkzk/flyui-react\" --filter=\"@gaconkzk/flyui-svelte\"",
    "build:docs": "pnpm build --filter=\"docs\"",
    "build:vue3": "pnpm build --filter=\"@gaconkzk/flyui-vue3\"",
    "build:react": "pnpm build --filter=\"@gaconkzk/flyui-react\"",
    "build:svelte": "pnpm build --filter=\"@gaconkzk/flyui-svelte\"",
    "publish:vue3": "pnpm publish --access=public --filter=\"@gaconkzk/flyui-vue3\"",
    "publish:react": "pnpm publish --access=public --filter=\"@gaconkzk/flyui-react\"",
    "publish:svelte": "pnpm run pub --filter=\"@gaconkzk/flyui-svelte\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaconkzk/flyui.git"
  },
  "author": "gaconkzk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaconkzk/flyui/issues"
  },
  "homepage": "https://github.com/gaconkzk/flyui#readme"
}
